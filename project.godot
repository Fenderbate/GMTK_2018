;   param=value ; assign values to parameters

config_version=3

[application]

config/name="GMTK_2018"
run/main_scene="res://src/Scenes/World.tscn"
config/icon="res://icon.png"

[editor_plugins]

enabled=PoolStringArray( "vnen.tiled_importer" )

[layer_names]

2d_physics/layer_1="Tower"
2d_physics/layer_2="Unit"
2d_physics/layer_3="Pickup"

[rendering]

environment/default_environment="res://default_env.tres"

[tiled_importer]

enable_json_format=true
